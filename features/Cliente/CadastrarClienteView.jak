    private void salvarMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_salvarMouseClicked
        
    	String nome = this.nome.getText();
	    String cpf = this.identificacao.getText();
	    String endereco = this.endereco.getText();
	    String email = this.email.getText();
	    String username = this.username.getText();
	    String senha = new String(this.senha.getPassword());
	    String dataStr = nascimento.getText();
	    SimpleDateFormat format = new SimpleDateFormat("dd/MM/yyyy");
	    java.sql.Date data;
        
        try {
            data = new java.sql.Date(format.parse(dataStr).getTime());
        } catch (ParseException ex) {
            Logger.getLogger(CadastrarClienteView.class.getName()).log(Level.SEVERE, null, ex);
        }
        //String nome, String identificacao, String endereco, String email, Date nascimento
        Pessoa pessoa = new Pessoa(nome, cpf, endereco, email);
        Login login = new Login(username, senha);
        
        mFuncionario = new Funcionario();
        mFuncionario.setLogin(login);
        mFuncionario.setDadosPessoa(pessoa);
        
        System.out.println("cadastrar funcionario\n"+mFuncionario.toString());
     
    }//GEN-LAST:event_salvarMouseClicked

