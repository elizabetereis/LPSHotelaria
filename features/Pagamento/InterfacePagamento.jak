    private void jToggleButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                               
        System.out.println("Pagando");
        String identificacao = textField6.getText();
        String descricao = textField2.getText();
        String dataStr = textField3.getText();
        
        double valorPago = Double.parseDouble(this.vlr_pago.getText());
        double valorConta = Double.parseDouble(this.vlr_total.getText());
        double desconto = Double.parseDouble(this.desconto.getText());
        double valorTotal = Double.parseDouble(this.Total_descontos.getText());
        
        SimpleDateFormat format = new SimpleDateFormat("dd/MM/yyyy");
	    java.sql.Date data;
        
	    FormaPagamento fp;
	    
        try {
            data = new java.sql.Date(format.parse(dataStr).getTime());
        } catch (ParseException ex) {
            Logger.getLogger(CadastrarClienteView.class.getName()).log(Level.SEVERE, null, ex);
        }
        
        if(cartao.isSelected())
        {
        	String titular = this.titular.getText();
        	String numeroCartao = this.numCartao.getText();
        	int codigoseguranca = Integer.parseInt(this.CodSeg.getText());
        	String dataStrValidade = this.validade.getText();
        	java.sql.Date dataValidade;
        	
            try {
            	dataValidade = new java.sql.Date(format.parse(dataStrValidade).getTime());
            } catch (ParseException ex) {
                Logger.getLogger(CadastrarClienteView.class.getName()).log(Level.SEVERE, null, ex);
            }
            
            
        	if(credito.isSelected())
        	{
        		int parcelas = Integer.parseInt(this.qtd_parcelas.getText());
        		double valorParcela = Double.parseDouble(this.vlr_pacelas.getText());
        		
        		fp = new PagamentoCartao(titular, numeroCartao, codigoseguranca, dataValidade, 
        				ETipoCartao.CREDITO, parcelas, valorParcela);
        	}
        	else
        	{
        		fp = new PagamentoCartao(titular, numeroCartao, codigoseguranca, dataValidade, 
        				ETipoCartao.DEBITO);
        	}
        	
        	

    	}
        else if(dinheiro.isSelected())
        {
        	double troco = Double.parseDouble(this.troco.getText());
        	fp = new PagamentoDinheiro(troco);
        }
        else
        {
        	fp = new PagamentoBoleto();
        }
        	
        Pagamento p = new Pagamento(data, valorConta, desconto, valorPago, 
    			descricao, fp);
        
        
    }                                              

